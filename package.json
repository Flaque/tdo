{
	"name": "todo",
	"version": "0.0.0",
	"description": "My rad module",
	"license": "MIT",
	"repository": "Flaque/todo",
	"author": {
		"name": "Flaque",
		"email": "econrad@zagmail.gonzaga.edu",
		"url": "flaque.net"
	},
	"bin": "cli.js",
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"start": "node cli.js",
		"test": "xo && ava",
		"lint": "xo",
		"fmt": "prettier --write *.js"
	},
	"files": ["cli.js", "ui.js"],
	"keywords": [""],
	"dependencies": {
		"chalk": "^2.4.1",
		"clear": "^0.1.0",
		"immutable": "^3.8.2",
		"ink": "^0.4.1",
		"ink-redux": "^1.0.0",
		"ink-select": "^1.2.0",
		"ink-select-flaque-patch": "^1.2.0",
		"ink-text-input": "^1.1.1",
		"meow": "^3.7.0",
		"prop-types": "^15.5.10",
		"redux": "^4.0.0",
		"uuid": "^3.2.1"
	},
	"devDependencies": {
		"ava": "^0.20.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"eslint": "^4.19.1",
		"eslint-config-prettier": "^2.9.0",
		"eslint-config-xo-react": "^0.13.0",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-react": "^7.7.0",
		"import-jsx": "^1.2.0",
		"js-yaml": "^3.11.0",
		"prettier": "1.12.1",
		"xo": "^0.20.3"
	},
	"babel": {
		"plugins": [
			[
				"transform-react-jsx",
				{
					"pragma": "h"
				}
			]
		]
	},
	"prettier": {
		"singleQuote": true
	},
	"ava": {
		"babel": {
			"presets": ["@ava/stage-4"],
			"plugins": [
				[
					"transform-react-jsx",
					{
						"pragma": "h",
						"useBuiltIns": true
					}
				]
			]
		}
	},
	"xo": {
		"extends": ["xo-react", "prettier"],
		"plugins": ["prettier"],
		"rules": {
			"react/no-unused-prop-types": 1,
			"react/jsx-tag-spacing": "off",
			"react/jsx-handler-names": "off"
		},
		"settings": {
			"react": {
				"pragma": "h"
			}
		}
	}
}
